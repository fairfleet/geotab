  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitest/coverage-c8": "^0.25.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "fast-glob": "^3.2.12",
    "nanoid": "^4.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "terser": "^5.15.1",
    "typescript": "^4.7.3",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
