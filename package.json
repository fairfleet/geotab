{
  "name": "@fairfleet/geotab",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "Fairfleet Gmbh",
  "module": "./index.mjs",
  "license": "MIT",
  "version": "0.1.4",
  "keywords": [
    "geotab",
    "api",
    "sdk",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "description": "An unofficial Geotab API client written in TypeScript",
  "contributors": [
    "Marvin Countryman <me@maar.vin>"
  ],
  "sideEffects": false,
  "scripts": {
